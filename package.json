{
  "name": "magnostic",
  "version": "0.0.4",
  "description": "Opinionated bundler-agnostic lightweight CSS-in-JS utility",
  "keywords": ["css-in-js", "framework-agnostic", "ssr", "css"],
  "repository": "https://github.com/tommywalkie/magnostic.git",
  "author": "tommywalkie <tommywalkie@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/**/*.ts --format esm,cjs --minify --dts",
    "test": "npm run build && uvu -r ts-node/register tests",
    "pack": "npm pack --dry-run"
  },
  "dependencies": {
    "nanoid": "^3.1.12",
    "stylis": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^14.10.2",
    "@types/picomatch": "^2.2.1",
    "@types/stylis": "^4.0.0",
    "esbuild": "^0.7.1",
    "picomatch": "^2.2.2",
    "ts-node": "^9.0.0",
    "tsup": "^3.7.0",
    "typescript": "^4.0.2",
    "uvu": "^0.3.3"
  }
}
